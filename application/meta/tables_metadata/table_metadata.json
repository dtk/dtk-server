{
  "node": {
    "order": [
      "id",
      "name",
      "node_type",
      "instance_id",
      "size",
      "os",
      "op_status",
      "dns_name"
    ],
    "mapping": {
      "name": "display_name",
      "region": "external_ref.region",
      "dns_name": "external_ref.dns_name",
      "size": "external_ref.size",
      "instance_id": "external_ref.instance_id",
      "node_type": "dtk_type",
      "zone": "external_ref.availability_zone",
      "os": "os_type",
      "op_status": "admin_op_status",
      "id": "dtk_id"
    }
  },
  "node_group": {
    "order": [
      "name",
      "id"
    ],
    "mapping": {
      "id": "dtk_id",
      "name": "display_name"
    }
  },
  "node_template": {
    "order": [
      "name",
      "id",
      "os",
      "template_type",
      "image_id",
      "size",
      "region"
    ],
    "mapping": {
      "id": "dtk_id",
      "name": "display_name",
      "os": "os_type",
      "template_type": "dtk_type",
      "image_id": "image_id",
      "size": "size",
      "region" : "region"
    }
  },
  "component": {
    "order": [
      "id",
      "name",
      "version"
    ],
    "mapping": {
      "name": "display_name",
      "version": "version",
      "library": "library.display_name",
      "id": "dtk_id",
      "library_id": "library_library_id",
      "component_type": "dtk_type"
    }
  },
  "attribute": {
    "order": [
      "id",
      "path",
      "value"
    ],
    "mapping": {
      "path": "display_name",
      "id": "dtk_id",
      "value": "value"
    }
  },
  "module": {
    "order": [
      "id",
      "name",
      "version"
    ],
    "mapping": {
      "name": "display_name",
      "id": "dtk_id",
      "version": "version"
    }
  },
  "library": {
    "order": [
      "id",
      "library_name"
    ],
    "mapping": {
      "library_name": "display_name",
      "id": "dtk_id"
    }
  },
  "remote_module": {
    "order": [
      "name",
      "version"
    ],
    "mapping": {
      "name": "display_name",
      "version": "version"
    }
  },
  "task": {
    "order": [
      "id",
      "status",
      "start",
      "end",
      "commit_msg"
    ],
    "mapping": {
      "id": "dtk_id",
      "end": "ended_at.get_date",
      "start": "started_at.get_date",
      "status": "status.upcase",
      "created": "created_at.get_date",
      "commit_msg": "commit_message"
    }
  },
  "target": {
    "order": [
      "id",
      "target_type",
      "iaas",
      "description"
    ],
    "mapping": {
      "iaas": "iaas_type",
      "id": "dtk_id",
      "description": "description",
      "target_type": "dtk_type"
    }
  },
  "assembly": {
    "order": [
      "id",
      "assembly_name",
      "status",
      "nodes",
      "components"
    ],
    "mapping": {
      "components": "nodes.first['components'].join(', ')",
      "nodes": "nodes.size",
      "status": "execution_status",
      "assembly_name": "display_name",
      "id": "dtk_id"
    }
  },
  "assembly_template": {
    "order": [
      "id",
      "assembly_name",
      "nodes",
      "components"
    ],
    "mapping": {
      "components": "nodes.first['components'].join(', ')",
      "nodes": "nodes.size",
      "assembly_name": "display_name",
      "id": "dtk_id"
    }
  },
  "module_diff": {
    "order": [
      "module_name",
      "module_id",
      "has_diff"
    ],
    "mapping": {
      "module_name": "name",
      "module_id": "dtk_id",
      "has_diff" : "has_diff ? 'TRUE' : ''"
    } 
  },
  "task_status": {
    "order": [   
       "task_type",
       "status",
       "node",
       "started_at",
       "ended_at",
       "errors"
    ],
    "mapping": {
       "task_type": "dtk_type",
       "status": "status",
       "node": "node.name",
       "started_at": "started_at.get_date",
       "ended_at": "ended_at.get_date",
       "errors": "errors.message"
   }
  },
  "assembly_task_status": {
    "order": [
       "task_id",
       "task_type",
       "status",
       "subtasks",
       "started_at",
       "ended_at"
    ],
    "mapping": {
       "task_id": "dtk_id",
       "task_type": "dtk_type",
       "status": "status",
       "subtasks": "subtasks.size",
       "started_at": "started_at.get_date",
       "ended_at": "ended_at.get_date"
   }
  },
  "service_add_on": {
    "order": [
       "id",
       "name",
       "description"
    ],
    "mapping": {
       "id": "dtk_id",
       "name": "display_name",
       "description": "description"
   }
  },
  "netstat_data": {
    "order": [   
       "node_name",
       "node_id",
       "local_address",
       "port",
       "protocol"
    ],
    "mapping": {
       "node_name": "node_name",
       "node_id": "node_id",
       "local_address": "local_address",
       "port": "port",
       "protocol" : "protocol"
   }
  }
}



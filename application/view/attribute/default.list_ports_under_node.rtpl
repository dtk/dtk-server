<div style="clear:both;"></div>
<br/><br/>
<div class="list_container">

	<div>
	<table class="list">
	<tr id="port-list-header-row">
			<th id="name-th"  class="">Port Name</th>
			<th id="value-th"  class="">Value</th>
	</tr>

	{%for port in port_list%}
		<tr id="list_row-{%=port%}" class="{%=[:class]%}">
				<td id="port_name-td-{%=port%}" class=""><a href="/xyz/attribute/display/{%=port[:id]%}">{%=port[:qualified_attribute_name_under_node]%}</a></td>
				<td id="value-td-{%=port%}" class="">{%=port[:value]%}</td>
		</tr>
	{%end%}
	</table>
	</div>
</div>

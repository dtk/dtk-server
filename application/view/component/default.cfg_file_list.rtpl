
<table class="display">
	<tr>
		<th>Name</th>
		<th>Owner</th>
		<th>Created By</th>
	</tr>
	{%for config_file in config_file_list%}
	<tr id="cfg-file-{%=config_file[:id]%}">
		<td><a href="javascript:R8.Displayview.loadCfgFile('{%=config_file[:id]%}');">{%=config_file[:name]%}</a></td>
		<td>{%=config_file[:owner_name]%}</td>
		<td>{%=config_file[:created_by_name]%}</td>
	</tr>
	{%end%}
</table>


<div id="details-wrapper" class="details-wrapper" style="">
	<div id="details-header" class="details-header" style="">
		<div class="title" style="">{%=component[:name]%}</div>
	</div>
	<div id="content-wrapper" class="content-wrapper" style="">
		<div id="details-content" class="content">
			<div class="display-container">
				<table class="display">
					<tr id="">
						<td class="label">Owner</td>
						<td class="field">R8</td>
					</tr>
					<tr id="">
						<td class="label">Description</td>
						<td class="field">{%=component[:description]%}</td>
					</tr>
					<tr id="">
						<td class="label">Date Created</td>
						<td class="field">{%=component[:created_at]%}</td>
					</tr>
					<tr id="">
						<td class="label">Type</td>
						<td class="field">{%=component[:basic_type]%}</td>
					</tr>
					<tr id="">
						<td class="label">Date Modified</td>
						<td class="field">{%=component[:updated_at]%}</td>
					</tr>
					<tr id="">
						<td class="label">&nbsp;</td>
						<td class="field">&nbsp;</td>
					</tr>
					<tr id="">
						<td class="label">OS Support</td>
						<td class="field">
							{%for os in component[:supported_os_list]%}
								<img title="{%=os[:name]%} : {%=os[:version]%}" src="{%=component_images_uri%}/{%=os[:ui][:images][:icon]%}" style=""/>
							{%end%}
						</td>
					</tr>
				</table>
				<br/><br/>
				<table class="display">
					<tr id="">
						<td class="label">Display Icon</td>
						<td class="field"><img src="{%=component_images_uri%}/{%=component[:ui][:images][:display]%}" style="float: left;"/></td>
					</tr>
				</table>
			</div>
		</div>

		<div id="instances-content" class="content" style="display: none;">
			This is some instances content
		</div>
		<div id="assembly-content" class="content" style="display: none;">
			This is some assembly content
		</div>
		<div id="editor-content" class="content" style="display: none;">
			This is some editor content
		</div>
		<div id="dependencies-content" class="content" style="display: none;">
			This is dependencies content
		</div>
		<div id="layout-content" class="content" style="display: none;">
			This is layout content
		</div>

	</div>
</div>

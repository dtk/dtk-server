

<div id="layout-editor" style="height: inherit; width: inherit;">
	<div id="editor-header" style="height: 40px; width: 100%"></div>
	<div id="editor-wrapper" style="height: 310px; width: 100%;">
		<div id="editor-fields" style="width: 200px; height: inherit; float: left;">
			<div style="height: 30px; width: inherit;">Available Fields</div>
			<ul id="available-fields" style="margin: 5px; width: 200px; height: 275px; overflow-y: scroll; float: left;">
			{%for field in field_def_list%}
				<li id="{%=field[:name]%}" style="height: 20px; width: 140px; margin: 3px; padding: 3px; border: 1px solid #999999;">
					<span style="">{%=field[:name]%}</span>
				</li>
			{%end%}
			</ul>
		</div>
		<div class="layout-editor body">

			<div id="" class="wspace-modal">
				<div id="-content" class="content">
					<div class="header">
						<div class="title">Create User</div>
					</div>
					<div class="body">
						<div class="tab-wrapper">
							<ul id="modal-tab-list" class="tab-list">
								<li id="group-1-tab" class="tab selected">Group 1</li>
							</ul>
							<div id="add-group-btn" class="add-group-btn"></div>
						</div>
						<div id="modal-content-wrapper" class="content-wrapper">
							<div id="group-1-content" class="tab-content">
								<ul id="group-1-field-list" style="margin: 5px; width: 200px; height: 200px; float: left; border: 1px solid black;">
								</ul>
							</div>
						</div>
					</div>
				</div>
			</div>

		</div>
	</div>
</div>
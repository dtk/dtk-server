<div class="display_container">
	<form id="component-display-form" action="" method="post" enctype="multipart/form-data">
		<table class="display">
		{%for field in field_list%}
			<tr><td class="label" nowrap="true"><b>{%=field[:attribute_name]%}</b></td></tr>
			<tr><td class="field">{%=field[:value]%}</td></tr>
        {%end%}
		</table>
		<input id="component-display-form-save" type="button" value="Edit"/>
	</form>
</div>

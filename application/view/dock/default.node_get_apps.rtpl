

<ul id="node-application-list" class="app-list list">
	{%for app in app_list%}
		<li id="app-{%=app[:id]%}" style="height: 40px;" class="app-item">
			<div style="float: left; height: 40px; width: 60px;">
				<img src="{%=_app[:base_images_uri]%}/componentIcons/{%=app[:component_icon_filename]%}" height="40" width="60"/>
			</div>
			<div style="float: left; margin: 10px 0 0 10px;">{%=app[:i18n]%}</div>
		</li>
	{%end%}
</ul>
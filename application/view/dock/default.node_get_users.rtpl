

<ul class="list">
	{%for user in user_list%}
		<li style="height: 40px;">
			<div class="avatar-small-bg">
				<img src="{%=_app[:avatar_base_uri]%}/{%=user[:avatar_filename]%}" class="avatar-small"/>
			</div>
			<div style="float: left; margin: 10px 0 0 10px;">{%=user[:username]%}</div>
		</li>
	{%end%}
</ul>
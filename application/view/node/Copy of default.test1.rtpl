

<div id="" style="overflow: hidden; display: block; position: absolute; height: 400px; width: 250px; border: 3px solid #CCCCCC; background-color: #DDDDDD;">
	<div id="wspace-dock-topbar">
		<div class="topbar-title-item">
			<div class="title">Components</div>
		</div>
<!--
		<div class="topbar-title-item">
			<div id="back-btn" class="back-btn"></div>
			<div class="title">Cloudera C3</div>
		</div>
-->
	</div>

	<div id="wspace-dock-body-container" style="position: relative; float:left; width: 500px; height: 375px;">
		<div id="wspace-dock-body-01" style="overflow-x: hidden; overflow-y: hidden; position: relative; float: left; height: 375px; width: 250px; background-color: #DDDDDD;">
			<div style="height: 365px; width: 240px; margin: 5px;">
			{%for component in component_list%}
				<div id="dock-component-{%=component[:id]%}" data-id="{%=component[:id]%}" class="wspace-dock-list-item {%=component[:css_class]%}">
					<div class="label">{%=component[:label]%}</div>
					<div class="more">&gt;</div>
				</div>
			{%end%}
			</div>
		</div>
	</div>
</div>


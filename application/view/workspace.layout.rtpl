		<div id="wspace-container">
			<div id="workspace" class="workspace">
				<div id="viewspace" class="viewspace">

					{%=viewspace%}

				</div>
			</div>
<!--
			<div id="wspace-rt-panel" style="display: none; position: absolute; height: 400px; width: 250px; border: 3px solid #CCCCCC; background-color: #DDDDDD; right: 0px; top: 0px;">
				<div id="wspace-rt-panel-topbar" style="float: left; position: relative; height: 30px; width: 100%; background-color: #FFFFFF;">
					<div id="wspace-rt-panel-close" class="close-tab-temp"></div>
				</div>
				<div id="wspace-rt-panel-body" style="overflow-x: hidden; overflow-y: scroll; position: relative; float: left; height: 360px; width: 240px;">
				</div>
			</div>
-->
			<div id="cmdbar-tabcontainer">
				<div id="cmdbar-tabs-wrapper">
					<div id="cmdbar-tabs">
					</div>
				</div>
				<div id="cmdbar-tab-content-wrapper"></div>
			</div>
		</div>

		<div id="cmdbar">
			<div class="cmdbar-input-wrapper">
				<form id="cmdbar_input_form" onsubmit="R8.Cmdbar.submit(); return false;">
					<input onfocus="if(this.value =='$:') this.value='';" onblur="if(this.value==='') this.value='$:';" type="text" value="$:" id="cmd" name="cmd" title="Enter Command"/>
				</form>
			</div>
		</div>

	</div>


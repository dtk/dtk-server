	<body onload="R8.Workspace.init();" class="yui3-skin-sam  yui-skin-sam">
	<div id="page-container" style="margin: 0 auto 0 auto;">
		<div id="page-topbar" class="top-bar">
			<div id="menu-bar" class="yui3-g menu-bar grey">
				<div class="yui3-u left-cap"></div>
				<div id="wspace-toolbar" class="toolbar yui3-u body">
					<div class="logo black"></div>
					<div class="item"><a href="{%=_app[:base_uri]%}/xyz/component/list">Library</a></div>
					<div class="item"><a href="{%=_app[:base_uri]%}/xyz/workspace" style="color: #4FA4D0">Workspace</a></div>
					<div class="item"><a href="{%=_app[:base_uri]%}/xyz/datacenter/list">Dashboard</a></div>
					<div class="item"><a href="{%=_app[:base_uri]%}/xyz/node/list">Inventory</a></div>
					<div class="divider"></div>
				</div>
				<div class="yui3-u right-cap"></div>
			</div>
			<div id="wspace-context-wrapper">
				<div class="l-endcap"></div>
				<div id="wspace-context"></div>
				<div class="r-endcap"></div>
			</div>
		</div>

<!--
		<div id="wspace-topbar">
			<div id="wspace-toolbar" class="toolbar">

				<div class="logo black"></div>

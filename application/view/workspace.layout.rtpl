</head>

	<body onload="R8.Workspace.init();" class="yui3-skin-sam  yui-skin-sam">
	<div id="page-container" style="margin: 0 auto 0 auto;">
		<div id="toolbar">
			&nbsp;&nbsp;<a href="javascript:R8.MainToolbar.toggleSlider();">Toggle</a>
			<div class="tbar_search"">
				<form id="tbar_search_form" onsubmit="R8.MainToolbar.search(); return false;">
					<input onfocus="if(this.value =='Search') this.value='';" onblur="if(this.value==='') this.value='Search';" type="text" value="Search" id="sq" name="sq" title="Enter Search Term"/>
					<input class="tbar_search_submit" onclick="R8.MainToolbar.search();" type="button" title="Search" value=""/>
				</form>
			</div>
		</div>
		<div id="wspace-container">
			<div id="workspace" class="workspace">
				<div id="viewspace" class="viewspace">



	<div id="group_01" data-id="01" data-model-name="group" class="basic-group group" style="position: relative; top: 100px; left: 100px; height: 120px; width: 856px;">
		<div class="basic-l-col">
			<div class="basic-tl-corner corner green"></div>
			<div class="basic-l-col-body green"></div>
			<div class="basic-bl-corner corner green"></div>
		</div>
		<div id="body_01" class="basic-body green" style="height: 116px; width: 592px;">
			<div style="height: 15px; width: 100%; float: left;">
				<h1>App Tier</h1>
			</div>
		</div>
		<div id="elastic_01" class="elastic" style="height: 120px;">
			<div class="elastic-header"></div>
			<div class="elastic-body" style="height:108px;"></div>
			<div class="elastic-footer"></div>
		</div>
		<div id="elastic_01" class="elastic-node-body" style="height: 116px; width: 196px;"></div>
		<div class="basic-r-col">
			<div class="basic-tr-corner corner green"></div>
			<div class="basic-r-col-body green"></div>
			<div class="basic-br-corner corner green"></div>
		</div>
	</div>





				</div>
			</div>
			<div id="cmdbar-tabcontainer">
				<div id="cmdbar-tabs-wrapper">
					<div id="cmdbar-tabs">
					</div>
				</div>
				<div id="cmdbar-tab-content-wrapper"></div>
			</div>
		</div>
		<div id="commandbar">
			<div class="cmdbar-input-wrapper">
				<form id="cmdbar_input_form" onsubmit="R8.Cmdbar.submit(); return false;">
					<input onfocus="if(this.value =='$:') this.value='';" onblur="if(this.value==='') this.value='$:';" type="text" value="$:" id="cmd" name="cmd" title="Enter Command"/>
				</form>
			</div>
		</div>
	</div>

	<div id="js_include_container">
		{%for js_include in _app[:js_includes]%}
			<script type="text/javascript" src="{%=js_include%}"></script>
		{%end%}
		<script type="text/javascript">
		{%for js_exe in _app[:js_exe_list]%}
			{%=js_exe%}
		{%end%}
		</script>
	</div>

	</body>
</html>

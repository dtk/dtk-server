			<th id="display_name-th" width="20%" class="">display_name</th>
	</tr>

	{%for component in component_list%}
		<tr id="list_row-{%=component%}" class="{%=[:class]%}">
				<td id="display_name-td-{%=component%}" class=""><a href="/xyz/component/display/{%=component[:id]%}">{%=component[:display_name]%}</a></td>
		</tr>
	{%end%}
	</table>
	</div>
</div>
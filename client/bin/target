#!/usr/bin/env ruby
$: << "/usr/lib/ruby/1.8/" #TODO: put in to get around path problem in rvm 1.9.2 environment
require File.expand_path('../r8_client', File.dirname(__FILE__))
include R8::Client
load_command("target")

conn = Conn.new()

response_ruby_obj = TargetCommand.execute_from_cli(conn,ARGV)
if print = response_ruby_obj.render_data("hash_pretty_print")
  pp print
end

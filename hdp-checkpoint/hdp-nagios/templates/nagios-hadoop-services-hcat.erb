
# HCAT Checks 
define service {
        hostgroup_name          hcatserver
        use                     generic-service
        service_description     HCATALOG::HCATALOG Process down alert
        check_command           check_tcp!9933!-w 1 -c 1
        normal_check_interval   1
        retry_check_interval    1 
        max_check_attempts      5
}


---
rackup: config.ru
pid: <%= @thin_dir %>/pid/thin.pid
group: <%= @daemon_group %>
timeout: 30
socket:  <%= @thin_socket %>
log: <%= @thin_dir %>/log/thin.log
max_conns: 1024
require: []

max_persistent_conns: 512
environment: production
user: <%= @daemon_user %>
daemonize: true
chdir: <%= @app_dir %>

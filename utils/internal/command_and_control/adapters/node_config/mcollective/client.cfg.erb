topicprefix = /topic/
main_collective = mcollective
collectives = mcollective
libdir = /usr/share/mcollective/plugins
logfile = <%= logfile %>
loglevel = info

# Plugins
securityprovider = sshkey
plugin.sshkey.client.send_key = <%= mcollective_ssh_local_public_key %>
plugin.sshkey.client.private_key = <%= mcollective_ssh_local_private_key %>
plugin.sshkey.client.authorized_keys = <%= mcollective_ssh_local_authorized_keys %>

connector = stomp_em
plugin.stomp.host = <%= stomp_host %>
plugin.stomp.port = 6163
plugin.stomp.user = mcollective
plugin.stomp.password = marionette

# Facts
factsource = yaml
plugin.yaml = /etc/mcollective/facts.yaml

